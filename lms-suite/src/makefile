clean_debug: 
	rm -f $(OBJ_DEBUGLINUX) $(OUT_DEBUGLINUX)
	rm -rf bin
	rm -rf $(OBJDIR_DEBUGLINUX)/resources
	rm -rf $(OBJDIR_DEBUGLINUX)/src
	rm -rf $(OBJDIR_DEBUGLINUX)/gui_src
	rm -rf $(OBJDIR_DEBUGLINUX)/include/CyAPI
	rm -rf $(OBJDIR_DEBUGLINUX)/src/lms6
	rm -rf $(OBJDIR_DEBUGLINUX)/gui_src/fft/oglGraph
	rm -rf $(OBJDIR_DEBUGLINUX)/gui_src/fft/oglGraph/glew
	rm -rf $(OBJDIR_DEBUGLINUX)/gui_src/fft
	rm -rf $(OBJDIR_DEBUGLINUX)/gui_src/lms6

before_release: 
	test -d bin || mkdir -p bin
	test -d $(OBJDIR_RELEASELINUX)/resources || mkdir -p $(OBJDIR_RELEASELINUX)/resources
	test -d $(OBJDIR_RELEASELINUX)/src || mkdir -p $(OBJDIR_RELEASELINUX)/src
	test -d $(OBJDIR_RELEASELINUX)/gui_src || mkdir -p $(OBJDIR_RELEASELINUX)/gui_src
	test -d $(OBJDIR_RELEASELINUX)/include/CyAPI || mkdir -p $(OBJDIR_RELEASELINUX)/include/CyAPI
	test -d $(OBJDIR_RELEASELINUX)/src/lms6 || mkdir -p $(OBJDIR_RELEASELINUX)/src/lms6
	test -d $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph || mkdir -p $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph
	test -d $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph/glew || mkdir -p $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph/glew
	test -d $(OBJDIR_RELEASELINUX)/gui_src/fft || mkdir -p $(OBJDIR_RELEASELINUX)/gui_src/fft
	test -d $(OBJDIR_RELEASELINUX)/gui_src/lms6 || mkdir -p $(OBJDIR_RELEASELINUX)/gui_src/lms6

after_release: 

release: before_release out_release after_release

out_release: before_release $(OBJ_RELEASELINUX) $(DEP_RELEASELINUX)
	$(LD) $(LIBDIR_RELEASELINUX) -o $(OUT_RELEASELINUX) $(OBJ_RELEASELINUX)  $(LDFLAGS_RELEASELINUX) $(LIB_RELEASELINUX)

