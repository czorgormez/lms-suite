clean_release: 
	rm -f $(OBJ_RELEASELINUX) $(OUT_RELEASELINUX)
	rm -rf bin
	rm -rf $(OBJDIR_RELEASELINUX)/resources
	rm -rf $(OBJDIR_RELEASELINUX)/src
	rm -rf $(OBJDIR_RELEASELINUX)/gui_src
	rm -rf $(OBJDIR_RELEASELINUX)/include/CyAPI
	rm -rf $(OBJDIR_RELEASELINUX)/src/lms6
	rm -rf $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph
	rm -rf $(OBJDIR_RELEASELINUX)/gui_src/fft/oglGraph/glew
	rm -rf $(OBJDIR_RELEASELINUX)/gui_src/fft
	rm -rf $(OBJDIR_RELEASELINUX)/gui_src/lms6

.PHONY: before_debug after_debug clean_debug before_release after_release clean_release

