<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="LMS6002_GUI_Main">
		<bg>wxSYS_COLOUR_MENUBAR</bg>
		<id_arg>0</id_arg>
		<style>wxSTATIC_BORDER</style>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>1</cols>
			<growablecols>0</growablecols>
			<growablerows>1</growablerows>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer1" member="no">
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON2" variable="btnOpen" member="yes">
							<label>Open</label>
							<handler function="OnmnuOpenSelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON3" variable="btnSave" member="yes">
							<label>Save</label>
							<handler function="OnmnuSaveAsSelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON4" variable="Chip2GUI" member="yes">
							<label>Chip-&gt;GUI</label>
							<handler function="OnmnuChipToGUISelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON5" variable="GUI2Chip" member="yes">
							<label>GUI-&gt;Chip</label>
							<handler function="OnmnuGUIToChipSelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON6" variable="btnRegisters" member="yes">
							<label>Registers Test</label>
							<handler function="OnRegisterTestSelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON7" variable="btnRefClk" member="yes">
							<label>Reference Clk...</label>
							<handler function="OnmnuRefClkSelected" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxBOTTOM|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxNotebook" name="ID_LMS6_TABS" variable="pcSPIS" member="yes">
					<style>wxNO_FULL_REPAINT_ON_RESIZE</style>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_SYSTEM_TAB" subclass="pnlSystem" variable="m_system" member="yes" />
						<label>System</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_TOP_TAB" subclass="pnlTop" variable="m_top" member="yes">
							<style>wxTAB_TRAVERSAL|wxFULL_REPAINT_ON_RESIZE</style>
						</object>
						<label>Top</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_TXPLL_TAB" subclass="pnlTxPLL" variable="m_TxPLL" member="yes" />
						<label>TxPLL+DSM</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_RXPLL_TAB" subclass="pnlTxPLL" variable="m_RxPLL" member="yes" />
						<label>RxPLL+DSM</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_TXLPF_TAB" subclass="pnlTxLPF" variable="m_TxLPF" member="yes" />
						<label>TxLPF</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_TXRF_TAB" subclass="pnlTxRF" variable="m_TxRF" member="yes" />
						<label>TxRF</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_RXLPF_TAB" subclass="pnlTxLPF" variable="m_RxLPF" member="yes" />
						<label>RxLPF</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_RXVGA2_TAB" subclass="pnlRxVGA2" variable="m_RxVGA2" member="yes" />
						<label>RxVGA2</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_RXFE_TAB" subclass="pnlRxFE" variable="m_RxFE" member="yes" />
						<label>RxFE</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_ADDC_TAB" subclass="pnlADDC" variable="m_ADDC" member="yes" />
						<label>ADDC</label>
					</object>
				</object>
				<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>3</cols>
					<growablecols>0</growablecols>
					<growablerows>0</growablerows>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="m_messageLogt" member="yes">
							<minsize>400,60</minsize>
							<style>wxTE_AUTO_SCROLL|wxTE_MULTILINE</style>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="btnShowLog" member="yes">
							<label>Log</label>
							<size>45,-1</size>
							<handler function="OnbtnShowLogClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
					<cols>3</cols>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT1" variable="lblVerRevMask" member="yes">
							<label>Ver:  Rev:  Mask:  </label>
							<size>200,-1</size>
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="lblRefClk" member="yes">
							<label>Reference Clock: 30.72 MHz</label>
							<size>175,-1</size>
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT2" variable="lblDevFwPr" member="yes">
							<label>Device: Firmware: Protocol:</label>
							<hidden>1</hidden>
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
