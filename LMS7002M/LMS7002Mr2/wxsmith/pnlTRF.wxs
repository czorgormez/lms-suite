<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="pnlTRF">
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>2</cols>
			<hgap>5</hgap>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="UNGROUPEDSizer" member="no">
					<label>UNGROUPED</label>
					<object class="sizeritem">
						<object class="wxFlexGridSizer" variable="UNGROUPED1Sizer" member="no">
							<cols>2</cols>
							<hgap>5</hgap>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText0" member="yes">
									<label>High pass pole freqency</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_EN_LOWBWLOMX_TMX_TRF" subclass="StdCombobox" variable="cmbEN_LOWBWLOMX_TMX_TRF" member="yes">
									<tooltip>Controls the high pass pole frequency of the RC biasing the gate of the mixer switches</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText1" member="yes">
									<label>Enable daisy chain LO buffer</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_EN_NEXTTX_TRF" subclass="StdCombobox" variable="cmbEN_NEXTTX_TRF" member="yes">
									<tooltip>Enables the daisy change LO buffer going from TRF&amp;1 to TRF2</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText2" member="yes">
									<label>Enable TXPAD power detector preamplifier</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_EN_AMPHF_PDET_TRF" subclass="StdCombobox" variable="cmbEN_AMPHF_PDET_TRF" member="yes">
									<tooltip>Enables the TXPAD power detector preamplifier</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText3" member="yes">
									<label>Switched capacitor at TXPAD output</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_F_TXPAD_TRF" subclass="StdCombobox" variable="cmbF_TXPAD_TRF" member="yes">
									<tooltip>controls the switched capacitor at the TXPAD output. Is used for tine tuning of the TXPAD output</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText4" member="yes">
									<label>Loss of loopback path at TX side</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_L_LOOPB_TXPAD_TRF" subclass="StdCombobox" variable="cmbL_LOOPB_TXPAD_TRF" member="yes">
									<tooltip>Controls the loss of the of the loopback path at the TX side</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText5" member="yes">
									<label>TXPAD linearizing part gain</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_LOSS_LIN_TXPAD_TRF" subclass="StdCombobox" variable="cmbLOSS_LIN_TXPAD_TRF" member="yes">
									<tooltip>Controls the gain of the linearizing part of of the TXPAD</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText6" member="yes">
									<label>TXPAD gain and output power</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_LOSS_MAIN_TXPAD_TRF" subclass="StdCombobox" variable="cmbLOSS_MAIN_TXPAD_TRF" member="yes">
									<tooltip>Controls the gain  output power of the TXPAD</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT8" variable="StaticText7" member="yes">
									<label>Bias voltage at gate of TXPAD cascade</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_VGCAS_TXPAD_TRF" subclass="StdCombobox" variable="cmbVGCAS_TXPAD_TRF" member="yes">
									<tooltip>Controls the bias voltage at the gate of TXPAD cascade</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT9" variable="StaticText8" member="yes">
									<label>Bias at gate of mixer NMOS</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_LOBIASN_TXM_TRF" subclass="StdCombobox" variable="cmbLOBIASN_TXM_TRF" member="yes">
									<tooltip>Controls the bias at the gate of the mixer NMOS</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT10" variable="StaticText9" member="yes">
									<label>Bias at gate of mixer PMOS</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxComboBox" name="ID_LOBIASP_TXX_TRF" subclass="StdCombobox" variable="cmbLOBIASP_TXX_TRF" member="yes">
									<tooltip>Controls the bias at the gate of the mixer PMOS</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxCheckBox" name="ID_SEL_BAND1_TRF" variable="chkSEL_BAND1_TRF" member="yes">
									<label>Enable signal for TXFE, band 1</label>
									<tooltip>Enable signal for TXFE, band 1</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxCheckBox" name="ID_SEL_BAND2_TRF" variable="chkSEL_BAND2_TRF" member="yes">
									<label>Enable signal for TXFE, band 2</label>
									<tooltip>Enable signal for TXFE, band 2</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
							<object class="sizeritem">
								<object class="wxCheckBox" name="ID_EN_LOOPB_TXPAD_TRF" variable="chkEN_LOOPB_TXPAD_TRF" member="yes">
									<label>Enable TXPAD loopback path</label>
									<tooltip>Enables the TXPAD loopback path</tooltip>
									<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
							</object>
						</object>
						<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
					</object>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>1</cols>
					<object class="sizeritem">
						<object class="wxStaticBoxSizer" variable="PowerdetectorSizer" member="no">
							<label>Power detector</label>
							<object class="sizeritem">
								<object class="wxFlexGridSizer" variable="Powerdetector2Sizer" member="no">
									<cols>2</cols>
									<hgap>5</hgap>
									<object class="sizeritem">
										<object class="wxStaticText" name="ID_STATICTEXT11" variable="StaticText10" member="yes">
											<label>Resistive load</label>
										</object>
										<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxComboBox" name="ID_LOADR_PDET_TRF" subclass="StdCombobox" variable="cmbLOADR_PDET_TRF" member="yes">
											<size>160,21</size>
											<tooltip>Controls the resistive load of the Power detector</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticBoxSizer" variable="BiascurrentSizer" member="no">
							<label>Bias current</label>
							<object class="sizeritem">
								<object class="wxFlexGridSizer" variable="Biascurrent4Sizer" member="no">
									<cols>2</cols>
									<hgap>5</hgap>
									<object class="sizeritem">
										<object class="wxStaticText" name="ID_STATICTEXT12" variable="StaticText11" member="yes">
											<label>Linearization section</label>
										</object>
										<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxComboBox" name="ID_ICT_LIN_TXPAD_TRF" subclass="StdCombobox" variable="cmbICT_LIN_TXPAD_TRF" member="yes">
											<tooltip>Control the bias current of the linearization section of the TXPAD</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxStaticText" name="ID_STATICTEXT13" variable="StaticText12" member="yes">
											<label>Main gm section</label>
										</object>
										<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxComboBox" name="ID_ICT_MAIN_TXPAD_TRF" subclass="StdCombobox" variable="cmbICT_MAIN_TXPAD_TRF" member="yes">
											<tooltip>Control the bias current of the main gm section of the TXPAD</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_COMBOBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticBoxSizer" variable="PowerdowncontrolsSizer" member="no">
							<label>Power down controls</label>
							<object class="sizeritem">
								<object class="wxFlexGridSizer" variable="Powerdowncontrols3Sizer" member="no">
									<cols>1</cols>
									<object class="sizeritem">
										<object class="wxCheckBox" name="ID_PD_PDET_TRF" variable="chkPD_PDET_TRF" member="yes">
											<label>Power detector</label>
											<tooltip>Powerdown signal for Power Detector</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxCheckBox" name="ID_PD_TLOBUF_TRF" variable="chkPD_TLOBUF_TRF" member="yes">
											<label>TX LO buffer</label>
											<tooltip>Powerdown signal for TX LO buffer</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxCheckBox" name="ID_PD_TXPAD_TRF" variable="chkPD_TXPAD_TRF" member="yes">
											<label>TXPAD</label>
											<tooltip>Powerdown signal for TXPAD</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
									<object class="sizeritem">
										<object class="wxCheckBox" name="ID_EN_G_TRF" variable="chkEN_G_TRF" member="yes">
											<label>Enable individual power downs</label>
											<tooltip>Enable control for all the TRF&amp;1 power downs</tooltip>
											<handler function="ParameterChangeHandler" entry="EVT_CHECKBOX" />
										</object>
										<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
									</object>
								</object>
								<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
							</object>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxRadioBox" name="ID_GCAS_GNDREF_TXPAD_TRF" variable="rgrGCAS_GNDREF_TXPAD_TRF" member="yes">
							<label>TXPAD cascode transistor gate bias</label>
							<content>
								<item>VDD</item>
								<item>GNDS</item>
							</content>
							<dimension>2</dimension>
							<tooltip>Controls if the TXPAD cascode transistor gate bias is referred to VDD or GND</tooltip>
							<style>wxRA_SPECIFY_COLS</style>
							<handler function="ParameterChangeHandler" entry="EVT_RADIOBOX" />
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
